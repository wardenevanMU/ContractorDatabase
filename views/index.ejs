<html>
<body>
  <h1>Welcome <%= agent_email%></h1>
  <% if (data && data.length > 0) { %>
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Company</th>
          <th>Items</th>
          <th>Location</th>
          <th>Start Time</th>
          <th>End Time</th>
          <th>Date</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(function(item) { %>
          <tr>
            <td><%= item.id %></td>
            <td><%= item.first_name %></td>
            <td><%= item.last_name %></td>
            <td><%= item.company %></td>
            <td><%= item.items %></td>
            <td><%= item.location %></td>
            <td><%= item.start_time %></td>
            <td><%= item.end_time %></td>
            <td><%= item.date %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
  <% } else { %>
    <p>No data available.</p>
  <% } %>

  <a href="/logout">Sign Out</a>
</body>
</html>
