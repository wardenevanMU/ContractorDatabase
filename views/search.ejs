<html>
  <body>
    <h1>Search Results for <%= searchQuery %></h1>
    <% if (data && data.length > 0) { %>
      <table>
        <thead>
          <tr>
            <th>Name</th>
            <th>Company</th>
            <th>Items</th>
            <th>Location</th>
            <th>Start Time</th>
            <th>End Time</th>
            <th>Date</th>
          </tr>
        </thead>
        <tbody>
          <% data.forEach(function(item) { %>
            <tr>
              <td><%= item.name %></td>
              <td><%= item.company %></td>
              <td><%= item.items %></td>
              <td><%= item.location %></td>
              <td><%= item.start_time %></td>
              <td><%= item.end_time %></td>
              <td><%= item.date %></td>
            </tr>
          <% }); %>
        </tbody>
      </table>
    <% } else { %>
      <p>No matching data found.</p>
    <% } %>
    <a href="/">Go back to Home</a>
  </body>
</html>
